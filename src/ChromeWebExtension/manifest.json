{
  "manifest_version": 2,
  "name": "Jabra Browser Integration Extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.5.0",
  "description": "Jabra Browser Integration Extension",
  "content_scripts": [
    {
      "js": [ "content-script.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Jabra Browser Integration Extension",
    "scripts": [ "popup-scripts.js" ]
  },
  "web_accessible_resources": [ "jabra-logo.png" ],
  "permissions": [ "nativeMessaging" ]
}